{
  "name": "kisphp/markdown-parser",
  "description": "Kisphp Markdown parser. Highly extensible and customizable markdown parser",
  "keywords": ["markdown", "parser", "kisphp"],
  "homepage": "http://www.kisphp.com",
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Marius Bogdan Rizac",
      "email": "mariusbogdan83@gmail.com",
      "homepage": "http://www.kisphp.com"
    }
  ],
  "require": {
    "php": ">=7.1"
  },
  "require-dev": {
    "phpunit/phpunit": "~7.5",
    "symfony/var-dumper": "~4.2",
    "friendsofphp/php-cs-fixer": "~2.13"
  },
  "autoload": {
    "psr-4": {
      "Kisphp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Kisphp\\": "tests/"
    }
  }
}
